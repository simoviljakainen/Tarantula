body {
    margin: 0pt;
    padding: 0pt;
    background: white none repeat scroll 0%;
    -moz-background-clip: initial;
    -moz-background-origin: initial;
    -moz-background-inline-policy: initial;
    font-family: Trebuchet MS, Arial, sans-serif;
    font-size:  14px;
}


#main{
    height:502px;
    width:1022px;
    float:left;
}


.x-form-item {
    font-family: Trebuchet MS, Arial, sans-serif;
    font-size:  14px;
    margin-top: 6px;
    margin-bottom: 6px;
}


/* Button default color (login page) */
.x-btn-center {
    color: #000;
}


.ext-strict{
    background-color: white;
}

td, th {
    font-family: Trebuchet MS, Arial, sans-serif;
    font-size:  14x;
}
p {
    font-family: Trebuchet MS, Arial, sans-serif;
    font-size:  14px;
}

a, h1, h2, h3, h4, h5, h6 {
    font-family: Trebuchet MS, Arial, sans-serif;
}


.x-layout-panel-center .x-layout-panel-body {
    /* Fix scrolling issues in IE 7. Ticket #694
     *  - relatively positioned elements don't scroll properly inside
     *    overflow: auto elements if those elements doesn't also
     *    have position: relative.
     *    < http://www.webdeveloper.com/forum/showthread.php?t=114891 >
     *  - overflow: auto is set to this element in ext layout code inside.rhtml
     */
    position: relative;
}


/* Div containing form area, including toolbar */
#center1-div {
    padding: 0;
    margin: 0;
}

/* Div for form, excluding toolbar */
#content {
    padding: 0 0 40px 0;
    margin: 0;
}


/* Format item as hyperlink */
.emulate-link {
    color: #ea6e04;
    /* font-weight: bold;*/
    cursor: pointer;
}

.emulate-link:hover {
    color: #994700;
}



table.default {
    border-collapse: collapse;
}


/* For emulating ext style forms/fieldsets in server provided forms (import)*/

/*
  Add some margin for views, which use whole content area
  Margins are normally defined by ExtJs elements, but
  some views do not render ext defined margins (Doors import, dasboard)
  */

.tarantula-form, #monitor_boxes, #reports {
    margin: 16px 0 0 0;
    padding: 0 0 30px 0;
}

.label, .tarantula-form th, .tarantula-form label {
    color:#555555;
    font-size:12px;
    font-weight:bold;
}




a:link, a:visited {text-decoration: none; color: #ea6e04;}
a:hover {
    color: #994700;
}


/* DASHBOARD AND REPORTS */


.dashboard_item p.report-item {
    clear: left;
}

.dashboard_item h1.report-item, .dashboard_item h2.report-item,
#reports h2.report-item  {
    background:#000 url(<%= asset_path 'tarantula/menu_bg.gif' %>) repeat scroll left center;
    color: #fff;
