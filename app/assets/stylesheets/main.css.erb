    font-weight: bold;
    margin: 0 !important;
    padding: 5px;
    clear: both;
}

#reports h3.report-item  {
    font-size: 16px;
    font-weight: bold;
    margin: 0 !important;
    padding: 5px;
    clear: both;
}


